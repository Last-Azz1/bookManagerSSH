<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pers.ssh.domain">
    <class name="Reader" table="reader">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="readername"></property>
        <property name="readertype"></property>
        <property name="sex"></property>
        <property name="tel"></property>
        <property name="max_num"></property>
        <property name="days_num"></property>
    </class>
</hibernate-mapping>